<core:FragmentDefinition
	xmlns="sap.m"
	xmlns:core="sap.ui.core">
	<SelectDialog

		
		noDataText="No Tasks Found"
		title="Select Task"
		
		search="onSearchSelectTaskValueHelp"
		confirm="onSelectTask"
		cancel="onDialogClose"
		multiSelect="true"
		items="{/}">
		<StandardListItem
			title="{attributes/name}"
			description ="{id}"
			iconDensityAware="false"
			iconInset="false"
			type="Active" />
	</SelectDialog>
</core:FragmentDefinition>
