<core:FragmentDefinition xmlns="sap.m" xmlns:core="sap.ui.core">
	<SelectDialog noDataText="No Users Found" title="Select Participants" search="onSearchParticipantValueHelp" confirm="onSelectUser"
		cancel="onDialogCloseParticipant" multiSelect="false" items="{ path: '/' }">
		<StandardListItem title="{firstName} {lastName}" description="{rate_card}">
			<customData>
				<core:CustomData key="key" value="{id}"/>
			</customData>
		</StandardListItem>
	</SelectDialog>
</core:FragmentDefinition>

