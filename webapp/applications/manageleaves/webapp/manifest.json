{
  "_version": "1.48.0",
  "sap.app": {
    "id": "VASPP.manageleaves",
    "type": "application",
    "i18n": "i18n/i18n.properties",
    "applicationVersion": {
      "version": "0.0.1"
    },
    "title": "{{appTitle}}",
    "description": "{{appDescription}}",
    "resources": "resources.json",
    "sourceTemplate": {
      "id": "@sap/generator-fiori:basic",
      "version": "1.8.3",
      "toolsId": "6185559e-214b-48ed-8e01-31a04470cf4d"
    },
    "dataSources": {
      "mainService": {
        "uri": "/sap/opu/odata/",
        "type": "OData",
        "settings": {
          "annotations": [],
          "localUri": "localService/metadata.xml",
          "odataVersion": "2.0"
        }
      }
    }
  },
  "sap.ui": {
    "technology": "UI5",
    "icons": {
      "icon": "",
      "favIcon": "",
      "phone": "",
      "phone@2": "",
      "tablet": "",
      "tablet@2": ""
    },
    "deviceTypes": {
      "desktop": true,
      "tablet": true,
      "phone": true
    }
  },
  "sap.ui5": {
    "flexEnabled": true,
    "dependencies": {
      "minUI5Version": "1.109.3",
      "libs": {
        "sap.m": {},
        "sap.ui.core": {},
        "sap.f": {},
        "sap.suite.ui.generic.template": {},
        "sap.ui.comp": {},
        "sap.ui.generic.app": {},
        "sap.ui.table": {},
        "sap.ushell": {}
      }
    },
    "contentDensities": {
      "compact": true,
      "cozy": true
    },
    "models": {
      "i18n": {
        "type": "sap.ui.model.resource.ResourceModel",
        "settings": {
          "bundleName": "VASPP.manageleaves.i18n.i18n"
        }
       
      },
     "": {
        "dataSource": "mainService",
        "preload": false,
        "settings": {}
      },
      "Leaves":{
			  "type": "sap.ui.model.json.JSONModel",
			  "uri": "model/Leaves.json"
			},
      "YearlyLeaves":{
			  "type": "sap.ui.model.json.JSONModel",
			  "uri": "model/YearlyLeaves.json"
			}
    },


    "resources": {
      "css": [
        {
          "uri": "css/style.css"
        }
      ]
    },
    "routing": {
      "config": {
        "routerClass": "sap.m.routing.Router",
        "viewType": "XML",
        "async": true,
        "viewPath": "VASPP.manageleaves.view",
        "controlAggregation": "pages",
        "controlId": "app",
        "clearControlAggregation": false
      },
      "routes": [
        {
          "name": "RouteApplyLeaves",
          "pattern": ":?query:",
          "target": [
            "TargetApplyLeaves"
          ]
        },
        {
          "name": "RouteAppliedLeaves",
          "pattern": "RouteAppliedLeaves",
          "target": [
            "TargetAppliedLeaves"
          ]
        },
        {
          "name": "RouteBalanceLeaves",
          "pattern": "RouteBalanceLeaves",
          "target": [
            "RouteBalanceLeaves"
          ]
        }
      ],
      "targets": {
        "TargetApplyLeaves": {
          "viewType": "XML",
          "transition": "slide",
          "clearControlAggregation": false,
          "viewId": "ApplyLeaves",
          "viewName": "ApplyLeaves"
        },
        "TargetAppliedLeaves": {
          "viewType": "XML",
          "transition": "slide",
          "clearControlAggregation": false,
          "viewId": "AppliedLeaves",
          "viewName": "AppliedLeaves"
        },
        "RouteBalanceLeaves": {
          "viewType": "XML",
          "transition": "slide",
          "clearControlAggregation": false,
          "viewId": "BalanceLeaves",
          "viewName": "BalanceLeaves"
        }
      }
    },
    "rootView": {
      "viewName": "VASPP.manageleaves.view.App",
      "type": "XML",
      "async": true,
      "id": "App"
    }
  }
}
